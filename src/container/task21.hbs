<div class="m-5">
    <div class="card p-3">
        {{#with heading}}
        {{#with main}}
        <div class="card">
            <div class="card-body">
                <div class="display-1">{{title}}</div>
                <div>
                    <p>{{text}}</p>
                </div>
                {{#with button_text}}
                <button class="btn btn-primary">{{solid}}</button>
                <br>
                <br>
                <br>
                <button class="btn btn-outline-primary">{{outline}}</button>
                {{/with}}
            </div>
        </div>
        {{/with}}
        <br>
        <br>
        {{#with second}}
        <div class="card">
            <div class="card-body">
                <div class="display-2">{{title}}</div>
                <div>
                    <p>{{text}}</p>
                </div>
                {{#with button_text}}
                <button class="btn btn-secondary">{{solid}}</button>
                <br>
                <br>
                <button class="btn btn-outline-secondary">{{outline}}</button>
                {{/with}}
            </div>
        </div>
        {{/with}}
        <br>
        <br>
        {{#with subblock}}
        <div class="card">
            <div class="card-body">
                <div class="display-3">{{title}}</div>
                <div>
                    <p>{{text}}</p>
                </div>
                {{#with button_text}}
                <button class="btn btn-dark">{{solid}}</button>
                <br>
                <br>
                <button class="btn btn-outline-dark">{{outline}}</button>
                {{/with}}
            </div>
        </div>
        {{/with}}
        {{/with}}
    </div>
    <br>
    <br>
    {{#with nested_display}}
    <div class="card">
        <div class="card-body">
            <div class="display-1">{{title}}</div>
            <br>
            {{#with children_display}}
            <div class="card">
                <div class="card-body">
                    <div class="display-4">{{title}}</div>
                    <div>
                        <p>{{text}}</p>
                    </div>
                    {{#with button}}
                    <button class="btn btn-outline-warning">{{text}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            {{/with}}
            <div class="display-6">{{header_text}}</div>
            <br>
            {{#with button}}
            <button class="btn btn-info">{{text}}</button>
            {{/with}}
        </div>
    </div>
    {{/with}}
    <br>
    <br>
    {{#with paragraph}}
    <div class="card" style="padding: 20px;">
        <div class="card-body">
            <div class="text-uppercase">{{title}}</div>
            <br>
            <div class="text-lowercase">{{text}}</div>
            <br>
            {{#with button}}
            <a href="{{url}}" class="btn btn-link">{{text}}</a>
            {{/with}}
        </div>
        <div class="card">
            <div class="card-body">
                {{#each list_name}}
                <ul>
                    <li> <a href="{{url}}" class="btn btn-link">{{text}}</a></li>
                </ul>
                {{/each}}
            </div>
        </div>
    </div>
    {{/with}}